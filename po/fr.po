#: After_the_Storm/episode1/scenarios/03_Civil_War.cfg:69
#, fuzzy
msgid "team_name^Elves (south-east)"
msgstr "team_name^Elfes (sud-est)"

#. [side]: id=Unidë, type=Elvish Avenger
#: After_the_Storm/episode1/scenarios/03_Civil_War.cfg:85
#, fuzzy
msgid "Unidë"
msgstr "Unidë"

#. [side]
#: After_the_Storm/episode1/scenarios/03_Civil_War.cfg:100
#, fuzzy
msgid "team_name^Fort Eldáin"
msgstr "team_name^Elynia"

#. [side]
#: After_the_Storm/episode1/scenarios/03_Civil_War.cfg:135
#, fuzzy
msgid "team_name^Grand Council"
msgstr "team_name^Galas"

